    <%= tank.name  %>
    <%= tank.description  %>
    <br>
    <%= tank.price_per_day %> | <%= tank.price_per_day %>
     <%#  <%= link_to 'Edit', edit_tanks_path(tank), class: "btn btn-outline-warning" %>
