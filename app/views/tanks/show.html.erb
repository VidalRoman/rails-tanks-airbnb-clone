
<div class="">
  <% Tank.all.each do |tank| %>
    <div class="tank-card">
      <%= tank.name  %>
      <%= tank.description  %>
      <br>
      <%= tank.price_per_day %>
      <%= tank.weight_in_kg %>
      <%= tank.manufacturer %>
      <%= tank.capacity %>
    </div>
    <%= tank.photos %>
  <% end %>
</div>
