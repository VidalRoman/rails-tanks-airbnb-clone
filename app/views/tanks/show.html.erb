<h1>You tanks</h1>
<div class="">
  <% Tank.all.each do |tank| %>
    <div class="tank-card">
      <%= tank.name  %>
      <%= tank.description  %>
      <br>
      <%= tank.price_per_day %>
      <%= tank.weight_in_kg %>
      <%= tank.manufacturer %>
      <%= tank.capacity %>
    </div>
    <% tank.photos.each do |photo| %>
      <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
    <% end %>
  <% end %>
</div>
